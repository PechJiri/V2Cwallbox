{
  "id": "v2c-wallbox",
  "name": {
    "en": "V2C Wallbox"
  },
  "class": "evcharger",
  "capabilities": [
    "measure_dynamic",
    "measure_charge_energy",
    "measure_charge_power",
    "measure_charge_state",
    "measure_charge_time",
    "measure_intensity",
    "measure_locked",
    "measure_paused",
    "measure_slave_error",
    "measure_voltage_installation",
    "car_connected"
  ],
  "capabilitiesOptions": {
    "measure_dynamic": {
      "title": {
        "en": "Dynamic Mode"
      }
    },
    "measure_charge_energy": {
      "title": {
        "en": "Charged energy (kWh)"
      },
      "units": {
        "en": "kWh"
      }
    },
    "measure_charge_power": {
      "title": {
        "en": "Current charging power (W)"
      },
      "units": {
        "en": "W"
      }
    },
    "measure_charge_state": {
      "title": {
        "en": "Charger state"
      },
      "values": [
        {
          "id": "0",
          "title": {
            "en": "EV not connected"
          }
        },
        {
          "id": "1",
          "title": {
            "en": "EV connected"
          }
        },
        {
          "id": "2",
          "title": {
            "en": "Charging"
          }
        }
      ]
    },
    "measure_charge_time": {
      "title": {
        "en": "Car connected (min)"
      },
      "units": {
        "en": "min"
      }
    },
    "measure_intensity": {
      "title": {
        "en": "Intensity (A)"
      },
      "units": {
        "en": "A"
      }
    },
    "measure_locked": {
      "title": {
        "en": "Locked"
      }
    },
    "measure_paused": {
      "title": {
        "en": "Paused"
      }
    },
    "measure_slave_error": {
      "title": {
        "en": "Inverter Communication Error"
      },
      "values": [
        {
          "id": "00",
          "title": {
            "en": "No Error"
          }
        },
        {
          "id": "01",
          "title": {
            "en": "Communication"
          }
        },
        {
          "id": "02",
          "title": {
            "en": "Reading"
          }
        },
        {
          "id": "03",
          "title": {
            "en": "Slave"
          }
        },
        {
          "id": "04",
          "title": {
            "en": "Waiting WiFi"
          }
        },
        {
          "id": "05",
          "title": {
            "en": "Waiting communication"
          }
        },
        {
          "id": "06",
          "title": {
            "en": "Wrong IP"
          }
        },
        {
          "id": "07",
          "title": {
            "en": "Slave not found"
          }
        },
        {
          "id": "08",
          "title": {
            "en": "Wrong Slave"
          }
        },
        {
          "id": "09",
          "title": {
            "en": "No response"
          }
        },
        {
          "id": "10",
          "title": {
            "en": "Clamp not connected"
          }
        }
      ]
    },
    "measure_voltage_installation": {
      "title": {
        "en": "Installation voltage"
      },
      "units": {
        "en": "V"
      }
    }
  },
  "images": {
    "small": "{{driverAssetsPath}}/images/small.png",
    "large": "{{driverAssetsPath}}/images/large.png",
    "xlarge": "{{driverAssetsPath}}/images/xlarge.png"
  },
  "platforms": [
    "local"
  ],
  "connectivity": [
    "lan"
  ],
  "pair": [
    {
      "id": "pair",
      "navigation": {
        "next": "list_my_devices"
      }
    },
    {
      "id": "list_my_devices",
      "template": "list_devices",
      "navigation": {
        "next": "add_my_devices",
        "prev": "pair"
      },
      "options": {
        "singular": true
      }
    },
    {
      "id": "add_my_devices",
      "template": "add_devices"
    }
  ],
  "flow": {
    "actions": [
      {
        "id": "set_paused",
        "title": {
          "en": "Set Charging State"
        },
        "titleFormatted": {
          "en": "Set Charging State to {{paused}}"
        },
        "args": [
          {
            "name": "paused",
            "type": "dropdown",
            "title": {
              "en": "Pause"
            },
            "values": [
              { "id": "1", "label": { "en": "Pause" } },
              { "id": "0", "label": { "en": "Charge" } }
            ]
          }
        ]
      },
      {
        "id": "set_locked",
        "title": {
          "en": "Set Charger Lock State"
        },
        "titleFormatted": {
          "en": "Set Charger Lock to {{locked}}"
        },
        "args": [
          {
            "name": "locked",
            "type": "dropdown",
            "title": {
              "en": "Lock"
            },
            "values": [
              { "id": "1", "label": { "en": "Lock" } },
              { "id": "0", "label": { "en": "Unlock" } }
            ]
          }
        ]
      },
      {
        "id": "set_intensity",
        "title": {
          "en": "Set Intensity"
        },
        "titleFormatted": {
          "en": "Set Intensity to {{intensity}} Amps"
        },
        "args": [
          {
            "name": "intensity",
            "type": "number",
            "title": {
              "en": "Intensity"
            },
            "min": 6,
            "max": 32
          }
        ]
      },
      {
        "id": "set_dynamic",
        "title": {
          "en": "Set Dynamic Mode"
        },
        "titleFormatted": {
          "en": "Set Dynamic Mode to {{dynamic}}"
        },
        "args": [
          {
            "name": "dynamic",
            "type": "dropdown",
            "title": {
              "en": "Dynamic Mode"
            },
            "values": [
              { "id": "1", "label": { "en": "Enable" } },
              { "id": "0", "label": { "en": "Disable" } }
            ]
          }
        ]
      },
      {
        "id": "set_min_intensity",
        "title": {
          "en": "Set Minimum Intensity For Dynamic Mode"
        },
        "titleFormatted": {
          "en": "Set Minimum Intensity For Dynamic Mode to {{MinIntensity}} Amps"
        },
        "args": [
          {
            "name": "MinIntensity",
            "type": "number",
            "title": {
              "en": "Minimum Intensity"
            },
            "min": 6,
            "max": 32
          }
        ]
      },
      {
        "id": "set_max_intensity",
        "title": {
          "en": "Set Maximum Intensity"
        },
        "titleFormatted": {
          "en": "Set Maximum Intensity For Dynamic Mode to {{MaxIntensity}} Amps"
        },
        "args": [
          {
            "name": "MaxIntensity",
            "type": "number",
            "title": {
              "en": "Maximum Intensity"
            },
            "min": 6,
            "max": 32
          }
        ]
      }
    ]
  }
}