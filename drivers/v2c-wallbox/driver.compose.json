{
  "id": "v2c-wallbox",
  "name": {
    "en": "V2C Wallbox"
  },
  "class": "evcharger",
  "platforms": ["local"],
  "connectivity": ["lan"],
  "capabilities": [
    "measure_dynamic",
    "measure_charge_energy", 
    "measure_charge_power",
    "measure_charge_state",
    "measure_charge_time",
    "measure_intensity",
    "measure_locked",
    "measure_paused",
    "measure_slave_error",
    "measure_voltage_installation"
  ],
  "images": {
    "small": "{{driverAssetsPath}}/images/small.png",
    "large": "{{driverAssetsPath}}/images/large.png",
    "xlarge": "{{driverAssetsPath}}/images/xlarge.png"
  },
  "pair": [
    {
      "id": "list_my_devices",
      "template": "list_devices",
      "navigation": {
        "next": "add_my_devices"
      }
    },
    {
      "id": "add_my_devices",
      "template": "add_devices"
    }
  ]
}